{
  "name": "temperature-manager",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "format:check": "prettier --check . --ignore-path .prettierignore",
    "format": "prettier --write . --ignore-path .prettierignore",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "preview": "vite preview",
